#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            // ... 他のキー設定 ...
        };

        // ... 他のレイヤー設定 ...
    };

    sensor-bindings {
        compatible = "zmk,keymap-sensor-bindings";
        bindings = <
            // 1番目のエンコーダー（例: 左手側）
            // 時計回り: Bキー、反時計回り: Aキー
            &inc_dec_kp A B

            // 2番目のエンコーダー（例: 右手側）
            // 時計回り: Dキー、反時計回り: Cキー
            &inc_dec_kp C D
        >;
    };
};